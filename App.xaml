<prism:PrismApplication
    x:Class="SpaceTradersWPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:prism="http://prismlibrary.com/">
    <prism:PrismApplication.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionaries/Animations.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/ButtonStyles.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/DatagridStyles.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/TextBlockStyles.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/FontSizes.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/DarkThemeColors.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/BorderStyles.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/TooltipStyles.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/ComboboxStyles.xaml" />
                <ResourceDictionary Source="Resources/Icons/Icons.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </prism:PrismApplication.Resources>
</prism:PrismApplication>
